{
  "name": "app",
  "private": true,
  "devDependencies": {
    "esbuild": "^0.25.5"
  },
  "scripts": {
    "build": "esbuild app/javascript/avo_calendar.js --bundle --sourcemap --format=esm --outdir=app/assets/builds --public-path=/assets --external:@hotwired/*",
